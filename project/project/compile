if [ $3 -eq 34 ]
then
export CUSTOM_IDD=td_tvt_kr24_in
fi

if [ $3 -eq 35 ]
then
export CUSTOM_IDD=td_tvt_ap_au27
fi

if [ $4 -eq 0 ]
then
export HAVE_3G=no_use_3g
fi

if [ $4 -eq 1 ]
then
export HAVE_3G=use_3g
fi

if [ $5 -eq 0 ]
then
export OSD_TWOLINES=no_osdtwolines
fi

if [ $5 -eq 1 ]
then
export OSD_TWOLINES=have_osdtwolines
fi

if [ $6 -eq 0 ]
then
echo "no onvif"
export SUPPROT_ONVIF=no_onvif
fi

if [ $6 -eq 1 ]
then
echo "support onvif"
export SUPPROT_ONVIF=support_onvif
fi

if [ $7 -eq 0 ]
then
echo "no nat"
export SUPPROT_NAT=no_nat
fi

if [ $7 -eq 1 ]
then
echo "support nat"
export SUPPROT_NAT=support_nat
fi

if [ $8 -eq 0 ]
then
echo "no push"
export SUPPROT_PUSH=no_push
fi

if [ $8 -eq 1 ]
then
echo "support push"
export SUPPROT_PUSH=support_push
fi

if [ $9 -eq 0 ]
then
echo "no api"
export SUPPROT_API=no_api
fi

if [ $9 -eq 1 ]
then
echo "support api"
export SUPPROT_API=support_api
fi

tput clear
tput cup 5 15
echo "同为数码软件打包工具"
tput cup 12 10
echo "嵌入式硬盘录像机固件《XDVR 3.0》编译及打包程序"

while true
do
	error_flag=0
	need_debug=0
	
	tput clear
	tput cup 5 10
	echo "编译模式 - 调试、测试、发布三种不同方式"	
	tput cup 7 20; echo "0: Cancel-----------------------退出"
	tput cup 9 20; echo "1: Debug------------------------调试版本"
	tput cup 11 20; echo "2: Test & cost down------------测试版本--------精简版"
	tput cup 13 20; echo "3: Test & Basic----------------测试版本--------基础版"
	tput cup 15 20; echo "4: Test & Professional---------测试版本--------专业版"
	tput cup 17 20; echo "5: Test & Ultimate-------------测试版本--------旗舰版"
	tput cup 19 20; echo "6: Release & cost down---------发布版本--------精简版"
	tput cup 21 20; echo "7: Release & Basic-------------发布版本--------基础版"
	tput cup 23 20; echo "8: Release & Professional------发布版本--------专业版"
	tput cup 25 20; echo "9: Release & Ultimate----------发布版本--------旗舰版"
	tput cup 27 20; echo "10: Clean----------------------清理所有"
	tput cup 29 10; echo -e "请选择编译模式 >_\b\c"
	read compile_mode

	#
	#case construct for checking the user selection
	#
	case $compile_mode in
		0)tput clear;exit 0;;
		1)error_flag=1;;
		2)error_flag=1;;
		3)error_flag=1;;
		4)error_flag=1;;
		5)error_flag=1;;
		6)error_flag=1;;
		7)error_flag=1;;
		8)error_flag=1;;
		9)error_flag=1;;
		10)error_flag=1;;
		*)./error 60 10  62
		tput cup 60 1 
		tput ed
	esac

	if [ $error_flag -eq 1 ]
	then
		error_flag=0
	
		tput cup 31 10; echo "你选择了编译模式 - $compile_mode"
		tput cup 33 10; echo "界面方案 - 工具条或者菜单形式 - 采用按钮形式或者锚形式"
		tput cup 35 20; echo "0: 退出"
		tput cup 37 20; echo "1: 工具条及按钮------TD_TVT_IN,TD_TVT_CN,TD_TVT_SP,TD_TVT_US02,TD_TVT_US47,TD_TVT_IL03,TD_TVT_RU04"
		tput cup 39 20; echo "2: 菜单及按钮--------TD_TVT_TW01, TD_TVT_IN25(ADI) TD_TVT_IN23 TD_TVT_CBC TD_TVT_IN_UI5(ADI)"
		tput cup 41 20; echo "3: 工具条及锚--------TD_TVT_BR27,TD_TVT_PL14,lv01"
		tput cup 43 20; echo "4: 菜单及锚----------TD_TVT_US47"
		tput cup 45 10; echo -e "请选择界面方案 >_\b\c"
		read ui_mode
	
		case $ui_mode in
			0)tput clear;exit 0;;
			1)export UI_MODE=ui_toolbar_button;error_flag=1;;
			2)export UI_MODE=ui_menu_button;error_flag=1;;
			3)export UI_MODE=ui_toolbar_anchor;error_flag=1;;
			4)export UI_MODE=ui_menu_anchor;error_flag=1;;
			*)./error 60 10 62
			tput cup 60 1 
			tput ed
		esac
		
		if [ $error_flag -eq 1 ]
		then
			error_flag=0	
			tput cup 46 10; echo "你选择了界面方案 - $ui_mode"
			tput cup 47 10; echo "客户端的界面选择："
			tput cup 48 20; echo "0: 退出"
			tput cup 49 20; echo "1: 浅蓝"
			tput cup 50 20; echo "2: 深蓝"
			tput cup 51 20; echo "3: 红色"
			tput cup 52 10; echo -e "请选择客户端的界面方案 >_\b\c"
			read ie_mode
			
			case $ie_mode in
				0)tput clear;exit 0;;
				1)error_flag=1;;
				2)error_flag=1;;
				3)error_flag=1;;
			esac		
			
			if [ $error_flag -eq 1 ]
			then
				echo "compile mode" $compile_mode "CUSTOMID=" $CUSTOM_IDD
				case $compile_mode in
				0)tput clear;exit 0;;
				1)need_debug=1 && cd ./project/               && make VERSION=debug   LEVEL=Ultimate     UI=${UI_MODE}	PACKID=${PACK_ID} CUSTOMID=${CUSTOM_IDD} HAVE3G=${HAVE_3G} OSDTWOLINES=${OSD_TWOLINES} SUPPROTAPI=${SUPPROT_API} ;exit 0;;
				2)need_debug=1 && cd ./project/ && make clean && make VERSION=test    LEVEL=CostDown     UI=${UI_MODE}	PACKID=${PACK_ID} CUSTOMID=${CUSTOM_IDD} HAVE3G=${HAVE_3G} OSDTWOLINES=${OSD_TWOLINES} SUPPROTAPI=${SUPPROT_API} && cd .. && ./pack $1 $2 ${ui_mode} $3 ${compile_mode} ${ie_mode} $4 $5 $6 ${7} ${8} ${9} ${need_debug}; exit 0;;
				3)need_debug=1 && cd ./project/ && make clean && make VERSION=test    LEVEL=Basic        UI=${UI_MODE}	PACKID=${PACK_ID} CUSTOMID=${CUSTOM_IDD} HAVE3G=${HAVE_3G} OSDTWOLINES=${OSD_TWOLINES} SUPPROTAPI=${SUPPROT_API} && cd .. && ./pack $1 $2 ${ui_mode} $3 ${compile_mode} ${ie_mode} $4 $5 $6 ${7} ${8} ${9} ${need_debug}; exit 0;;
				4)need_debug=1 && cd ./project/ && make clean && make VERSION=test    LEVEL=Professional UI=${UI_MODE}	PACKID=${PACK_ID} CUSTOMID=${CUSTOM_IDD} HAVE3G=${HAVE_3G} OSDTWOLINES=${OSD_TWOLINES} SUPPROTAPI=${SUPPROT_API} && cd .. && ./pack $1 $2 ${ui_mode} $3 ${compile_mode} ${ie_mode} $4 $5 $6 ${7} ${8} ${9} ${need_debug}; exit 0;;
				5)need_debug=1 && cd ./project/ && make clean && make VERSION=test    LEVEL=Ultimate     UI=${UI_MODE}	PACKID=${PACK_ID} CUSTOMID=${CUSTOM_IDD} HAVE3G=${HAVE_3G} OSDTWOLINES=${OSD_TWOLINES} SUPPROTAPI=${SUPPROT_API} && cd .. && ./pack $1 $2 ${ui_mode} $3 ${compile_mode} ${ie_mode} $4 $5 $6 ${7} ${8} ${9} ${need_debug}; exit 0;;
				6)cd ./project/ && make clean && make VERSION=release LEVEL=CostDown     UI=${UI_MODE}	PACKID=${PACK_ID} CUSTOMID=${CUSTOM_IDD} HAVE3G=${HAVE_3G} OSDTWOLINES=${OSD_TWOLINES} SUPPROTAPI=${SUPPROT_API} && cd .. && ./pack $1 $2 ${ui_mode} $3 ${compile_mode} ${ie_mode} $4 $5 $6 ${7} ${8} ${9} ${compile_mode}; exit 0;;
				7)cd ./project/ && make clean && make VERSION=release LEVEL=Basic        UI=${UI_MODE}	PACKID=${PACK_ID} CUSTOMID=${CUSTOM_IDD} HAVE3G=${HAVE_3G} OSDTWOLINES=${OSD_TWOLINES} SUPPROTAPI=${SUPPROT_API} && cd .. && ./pack $1 $2 ${ui_mode} $3 ${compile_mode} ${ie_mode} $4 $5 $6 ${7} ${8} ${9} ${compile_mode}; exit 0;;
				8)cd ./project/ && make clean && make VERSION=release LEVEL=Professional UI=${UI_MODE}	PACKID=${PACK_ID} CUSTOMID=${CUSTOM_IDD} HAVE3G=${HAVE_3G} OSDTWOLINES=${OSD_TWOLINES} SUPPROTAPI=${SUPPROT_API} && cd .. && ./pack $1 $2 ${ui_mode} $3 ${compile_mode} ${ie_mode} $4 $5 $6 ${7} ${8} ${9} ${compile_mode}; exit 0;;
				9)cd ./project/ && make clean && make VERSION=release LEVEL=Ultimate     UI=${UI_MODE}	PACKID=${PACK_ID} CUSTOMID=${CUSTOM_IDD} HAVE3G=${HAVE_3G} OSDTWOLINES=${OSD_TWOLINES} SUPPROTAPI=${SUPPROT_API} && cd .. && ./pack $1 $2 ${ui_mode} $3 ${compile_mode} ${ie_mode} $4 $5 $6 ${7} ${8} ${9} ${compile_mode}; exit 0;;
				#9)./pack $1 $2 ${ui_mode} $3 ${compile_mode} ${ie_mode} $4 $5 $6 ${7} ${8};exit 0;;
				10)cd ./project/ && make clean && cd ../ && rm ./*.tar;;
				*)./error 60 10 62
				tput cup 60 1 
				tput ed
				esac
			fi		
		fi	
		
	fi
done

exit 0
