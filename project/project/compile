then
export CUSTOM_IDD=td_tvt_sp
fi
if [ $3 -eq 4 ]
then
export CUSTOM_IDD=td_tvt_us02
fi
if [ $3 -eq 5 ]
then
export CUSTOM_IDD=td_tvt_ru04
fi
if [ $3 -eq 6 ]
then
export CUSTOM_IDD=td_tvt_br27
fi
if [ $3 -eq 7 ]
then
export CUSTOM_IDD=td_tvt_tw01
fi
if [ $3 -eq 8 ]
then
export CUSTOM_IDD=td_tvt_gb14
fi
if [ $3 -eq 9 ]
then
export CUSTOM_IDD=td_tvt_in23
fi
if [ $3 -eq 10 ]
then
export CUSTOM_IDD=td_tvt_us47
fi
if [ $3 -eq 11 ]
then
export CUSTOM_IDD=td_tvt_in25
fi
if [ $3 -eq 12 ]
then
export CUSTOM_IDD=td_tvt_pl14
fi
if [ $3 -eq 13 ]
then
export CUSTOM_IDD=td_tvt_tw01ts
fi
if [ $3 -eq 14 ]
then
