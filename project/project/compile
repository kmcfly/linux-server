cp ./product/lib_3520D_040/*.a ./project/lib/
cp ./product/lib_3520D_040/*.so ./project/lib/
export PACK_ID=packid_22
fi

sed -i "s/make -C/make -j5 -C/" ./project/Makefile


if [ $3 -eq 1 ]
then
export CUSTOM_IDD=td_tvt_in
fi
